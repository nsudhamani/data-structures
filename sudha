                <th class='text-left' style='width:73%; font-weight: normal;'>Exp. Name: <i
                        class='expTitle'>C program to create dynamic memory allocation using malloc( )
                </i></th>
                <th class='text-left' id="date" style='vertical-align:middle;width:15px'>Date:</th>
            </tr>
            </thead>
        </table>
    </div>
    <div class="subpage">
        
        
        <div class=""><b style='border-bottom:1px solid black;'>Aim:</b></div>
        
        <div class="questionText ql-editor" id="questionText"><p>Write a C program to create dynamic memory allocation using malloc( )</p>
        </div>
        
        
        <div style="margin-bottom: 1rem;"><b style='border-bottom:1px solid black;'>Source Code:</b></div>
        
        
        
        <div id="userSolution" style="padding: 0 15px 0 0;"></div>
        <div id='executionResultsParentDiv' style='min-width:200px; min-height:200px;'></div>
        
        
    </div>
</div>
<script>
    $(document).ready(function () {
        ltr();
        applyDynamics(".questionText");
    });

    function ltr() {
        $('#expTitleTag').text("C program to create dynamic memory allocation using malloc( )");
        $('#memberIdTag').text('22K61A05B8');
        showLoadingModal();
        arrowRotateEffect();
        initCode();
        var labName = 'Data Structures Using C Lab - 21CSCSL2080 - I Year - II Sem - 2022-2026';
        var qNo = '130';
        setTimeout(function () {
            hideLoadingModal();
            var $vertical = $('.vertical-text'), width = $vertical.css('width');
            $vertical.css('transform', "translate(10mm, -" + width + ") rotate(-90deg)");
            $vertical.css('display', 'none');
            $('.vertical-text-pageNo').css('display', 'none');
            
            
            showCExecutionResults(cExecutionResultsArr);
            
            generatePDF();
        }, 300);
    }

    //Once you create an instance like var preloader = new GSPreloader(), call preloader.active(true) to open it, preloader.active(false) to close it, and preloader.active() to get the current status.
    //Note: The below code should be here and not in some JS file, as it internally uses the current DOM and initializes some parent reference.
    var preloader = new GSPreloader({
        radius: 50,
        dotSize: 18,
        dotCount: 12,
        colors: ["#720099", "#CC7300", "#720099", "#CC7300"], //have as many or as few colors as you want.
        boxOpacity: 0.5,
        animationOffset: 1.5 //jump 1.5 seconds into the animation for a more active part of the spinning initially (just looks a bit better in my opinion)
    });

    function initCode() {
        var solutionContent = "";
        if (userSolution != null) {
            if (userSolution.hasOwnProperty('filesContentArr')) {
                $.each(userSolution.filesContentArr, function (fidx, fileEntry) {
                    $('#userSolution').append('<ul class="nav nav-tabs"><li><a class="lang-c" style="border-radius: 4px 4px 0 0; border: 1px solid black;">' + fileEntry['fileName'] + '</a></li></ul>'
                        + '<pre style="overflow: visible; border-radius: 0 4px 4px 4px; border: 1px solid black;" class="lang-c linenums1" id="userSolution' + fidx + '"></pre><br>');
                    $('#userSolution' + fidx).text(fileEntry['fileContent']);
                });
            } else if (userSolution.hasOwnProperty('content')) {
                solutionContent = "\n" + userSolution.content + "\n\n";
                $('#userSolution').text(solutionContent);
            } else if (userSolution.hasOwnProperty('essayAnswer')) {
                solutionContent = "\n" + userSolution.essayAnswer + "\n\n";
                $('#userSolution').text(solutionContent);
            }
        }
    }

    function getExecutionResultsParentDiv() {
        return 'executionResultsParentDiv';
    }

    function updateQuestionText() {
        var questionText = questionDBObject.questionText;
        if (questionDBObject.multiLanguageBoilerplate !== 'no-boiler') {
            questionText = generateMultiLangQuestionTextInfo(questionDBObject.multiLangParameters, questionDBObject.functionName, questionDBObject.returnType, language.toLowerCase(), questionDBObject["arrayElementType"]) + '<br><br>' + questionDBObject.questionText;
        }
        $("#questionText").html(questionText);
    }

    function showExecutionResultsWithoutComparisionWhenDoNotRunTestCases() {
        var allTablesText = "<div class='row'><div class='col-sm-12'><h3 style='margin-top: 7px; padding:10px;' class=' text-center well'>Execution Results"
            + "<span id='executionResultsStatusSpan' style='font-size:65%;'></span></h3><div id='executionResultTablesContainerDiv' class='col-sm-12'>";
        $('#' + getExecutionResultsParentDiv()).append(allTablesText + "</div></div></div>");
        $('#executionResultsStatusSpan').html(" - <i class='fa fa-check'></i>").css('color', 'green');
        $("#executionResultTablesContainerDiv").html("<div class='text-center darkShadow' style='padding: 4%; font-family: monospace; color: green;font-size: 16px;'>Program has been executed without test cases</div>");
    }

    function showCExecutionResults(cExecutionResultsArr) {
        var allTablesText = "<div class='row'><div class='col-sm-12'><h4 id='execResultsHeading' class=' text-center '>Execution Results"
            + "<span id='executionResultsStatusSpan' style='font-size:70%;'></span>"
            + "</h4>";
        allTablesText = allTablesText + "<div id='executionResultTablesContainerDiv' class='col-sm-12'>";
        $('#' + getExecutionResultsParentDiv()).append(allTablesText + "</div></div></div>");
        var allTestCasesPassed = true;

        $.each(cExecutionResultsArr, function (idx, executionResult) {
            var cExecutionFlowArr = executionResult['cExecutionFlowArr'];
            var testCaseText = "<div style='margin-bottom: 5%;'> " +
                "                   <div class='row execution-results-border'>" +
                "<div class='col-12 col-sm-12 p-2 text-center font-weight-bold'>Test Case - " + (idx + 1) + "</div>" +
                "</div>";
            if (executionResult["isJava"]) {
                testCaseText += "<div class='row execution-results-border'> <div class='col-12 col-sm-12'>Method Call</div></div>";
                testCaseText += "<div class='row execution-results-border'>" +
                    "<div class='col-12 col-sm-12'>" +
                    "<pre class='outputPre'>"
                    + executionResult["methodCallText"];
                if (executionResult.argsArr === "[ ]" || executionResult.argsArr === "") {
                    testCaseText += " - Arguments : None ";
                } else {
                    testCaseText += " - Arguments : " + executionResult.argsArr;
                }
                testCaseText += "</pre></div></div>";
            }
            testCaseText += "<div class='row execution-results-border'>" +
                "<div class='col-12 p-1 ml-2 font-weight-bold'>User Output</div></div>";
            var execResultsText = "";
            for (var execIndex = 0; execIndex < cExecutionFlowArr.length; execIndex++) {
                var entry = cExecutionFlowArr[execIndex];
                if (!entry.hasOwnProperty('userOutput')) {
                    entry['userOutput'] = entry['programOutput'];//copy the same as all test cases would have succeeded
                }
                if (entry.isUserInput) {
                    if (execResultsText.indexOf("__user__Input__") === -1) {
                        execResultsText = execResultsText + "<div class='row execution-results-border'><div class='col-6'>" +
                            "<span class='cmd' style='padding: 1px 4px' title='User Input'>" + entry.userInput + "</span></div></div>";
                    } else {
                        var inputText = "<span class='cmd' style='padding: 1px 4px' title='User Input'>" + entry.userInput + "</span>";
                        execResultsText = execResultsText.replace(/__user__Input__/g, inputText);
                    }
                } else if (entry.isProgramOutput && entry.programOutput) {
                    execResultsText = execResultsText + "<div class='row execution-results-border'>" +
                        "<div class='col-12'>" +
                        "<pre class='outputPre'>" + entry.programOutput + "";
                    execResultsText = execResultsText + "__user__Input__</pre></div></div>";
                }
            }
            execResultsText = execResultsText.replace(/__user__Input__/g, "");
            execResultsText = execResultsText + "";
            testCaseText = testCaseText + execResultsText;
            testCaseText = testCaseText + "</div>";
            $('#executionResultTablesContainerDiv').append(testCaseText);
        });
        $('#executionResultsStatusSpan').text(" - All test cases have succeeded!").css('color', 'green');
        return allTestCasesPassed;
    }

    function arrowRotateEffect() {
        $('#sectionContentDiv').on('show.bs.collapse', function () {
            $(this).siblings('.section-heading').removeClass('active');
        }).on('hide.bs.collapse', function () {
            $(this).siblings('.section-heading').addClass('active');
        });
    }

    function generatePDF() {
        window.PagedPolyfill.preview().then(() => {
            const fileName = (memberId ? memberId + "_" : "") + '130' + "_" + moment().format("YYYY_MM_DD");
            pagedJsPrintUtils.getPDFBlobFromPagedJSPage().then((blob) => {
                const url = pagedJsPrintUtils.downloadBlob(blob, fileName);
                Swal.fire('Info', 'Lab record generated as PDF successfully', 'info').then(() => {
                    URL.revokeObjectURL(url);
                    window.close();
                });
            }).catch((e) => {
                Swal.fire('Error', 'Encountered error with message: ' + e.message, 'error').then(() => {
                    window.close();
                });
            });
        }).catch(e => {
            Swal.fire('Error', 'Error encountered: ' + (e.message || e), 'error').then(() => {
                window.close();
            });
            console.dir(e);
        });
    }

    document.getElementById('date').innerHTML += date;
</script>
</body>
</html>